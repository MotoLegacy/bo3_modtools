                },
                "pain": {
                    "animation_selector": "pain@exposed",
                    "transitions": {
                        "locomotion": {
                            "animation_selector": "idle@exposed>locomotion"
                        }
                    }
                },
                "scan": {
                    "animation_selector": "scan@tactical_locomotion",
                    "loopsync": true,
                    "multipledelta": true,
                    "transitions": {
                        "locomotion": {
                            "animation_selector": "tactical_shoot@locomotion>move@locomotion"
