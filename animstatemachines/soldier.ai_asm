                "move": {
                    "animation_selector": "move@stealth",
                    "loopsync": true,
                    "cleanloop": true,
                    "multipledelta": true,
                    "transitions": {
                        "idle@stealth": {
                            "animation_selector": "arrive@stealth"
                        }
                    }
                },
                "react": {
                    "animation_selector": "react@stealth"
                },
