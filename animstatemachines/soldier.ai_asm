                        }
                    }
                },
                "charge_melee": {
                    "animation_selector": "charge_melee@exposed",
                    "transitions": {
                        "locomotion": {
                            "animation_selector": "idle@exposed>locomotion"
                        }
                    }
                },
                "idle": {
                    "animation_selector": "idle@exposed",
                    "transitions": {
                        "locomotion": {
                            "animation_selector": "idle@exposed>locomotion"
                        },
