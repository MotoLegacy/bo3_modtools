        "locomotion": {
            "substates": {
                "arrive": {
                    "animation_selector": "arrive@locomotion",
                    "transitions": {
                        "exposed": {
                            "animation_selector": "flank@exposed"
                        },
                        "tactical_locomotion": {
                            "animation_selector": "flank@exposed"
                        }
                    }
                },
                "arrive_exposed": {
                    "animation_selector": "arrive_exposed@locomotion",
                    "delta_layer_function": "evaluatehumanexposedarrivalanimations"
                },
                "charge_melee": {
                    "animation_selector": "charge_melee@locomotion"
                },
                "charge_melee_attack": {
                    "animation_selector": "charge_melee_attack@locomotion"
                },
                "inplace_pain": {
                    "animation_selector": "inplace_pain@locomotion"
                },
                "move": {
                    "animation_selector": "move@locomotion",
                    "loopsync": true,
                    "multipledelta": true,
                    "transitions": {
                        "tactical_locomotion": {
                            "animation_selector": "move@locomotion>tactical_locomotion"
                        }
                    }
                },
                "pain": {
                    "animation_selector": "pain@locomotion",
                    "delta_layer_function": "evaluateblockedanimations"
                },
                "shoot": {
                    "animation_selector": "shoot@locomotion",
                    "aim_selector": "aim_additive@locomotion",
                    "shoot_selector": "shoot_additive@locomotion",
                    "loopsync": true,
                    "cleanloop": true,
                    "multipledelta": true,
                    "allow_transdec_aim": true
                },
                "turn": {
                    "animation_selector": "turn@locomotion",
                    "delta_layer_function": "evaluatehumanturnanimations"
                }
            }
        },
        "patrol": {
            "substates": {
                "patrol_walk": {
                    "animation_selector": "patrol_walk@patrol",
                    "loopsync": true,
                    "cleanloop": true
                }
            }
        },
        "special_death": {
            "substates": {
                "death": {
                    "animation_selector": "death@special_death",
